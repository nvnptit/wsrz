# ==========================================================
# PHẦN MITM (All_In_One_7in1)
# ==========================================================
[MITM]
hostname = %APPEND% subscription.grammarly.com, spclient.wg.spotify.com, *spclient.spotify.com, -redirector*.googlevideo.com, *.googlevideo.com, www.youtube.com, s.youtube.com, youtubei.googleapis.com, apimboom2.globaldelight.net, api.revenuecat.com, api.alohaprofile.com

# ==========================================================
# PHẦN SCRIPT (All_In_One_7in1)
# ==========================================================
[Script]
# ---- Từ module: grammarly.module ----
Grammarly=type=http-response,pattern=^https?:\/\/subscription\.grammarly\.com\/api\/v1\/subscription$,script-path=https://raw.githubusercontent.com/nvnptit/wsrz/main/grammarly.js,requires-body=1

# ---- Từ module: spotify.module ----
spotify-json = type=http-request,pattern=^https:\/\/(spclient\.wg\.spotify\.com|.*-spclient\.spotify\.com(:443)?)\/(artistview\/v1\/artist|album-entity-view\/v2\/album)\/,requires-body=0,script-path=https://raw.githubusercontent.com/nvnptit/wsrz/main/spotify-json.js
spotify-proto = type=http-response,pattern=^https:\/\/(spclient\.wg\.spotify\.com|.*-spclient\.spotify\.com(:443)?)\/(bootstrap\/v1\/bootstrap|user-customization-service\/v1\/customize)$,requires-body=1,binary-body-mode=1,max-size=0,script-path=https://raw.githubusercontent.com/nvnptit/wsrz/main/spotify-proto.js,script-update-interval=0

# ---- Từ module: youtube.sgmodule ----
youtube.response = type=http-response,pattern=^https:\/\/youtubei\.googleapis\.com\/youtubei\/v1\/(browse|next|player|search|reel\/reel_watch_sequence|guide|account\/get_setting|get_watch),requires-body=1,max-size=-1,binary-body-mode=1,engine={{{Script execution engine}}},script-path=https://raw.githubusercontent.com/nvnptit/wsrz/main/youtube.response.js,argument="{"lyricLang":"{{{Lyric translation language}}}","captionLang":"{{{Subtitle translation language}}}","blockUpload":{{{Block upload button}}},"blockImmersive":{{{Block immersive button}}},"debug":{{{Enable debug mode}}}}"

# ---- Từ module: boom.module ----
Boom=type=http-response,pattern=^https:\/\/apimboom2\.globaldelight\.net\/itunesreceipt_v2\.php$,requires-body=1,script-path=https://raw.githubusercontent.com/nvnptit/wsrz/main/boom.js

# ---- Từ module: vsco.module ----
VSCO=type=http-request,pattern=^https?:\/\/api\.revenuecat\.com\/v\d\/subscribers\/,script-path=https://raw.githubusercontent.com/nvnptit/wsrz/main/vsco.js
VSCO1=type=http-response,pattern=^https?:\/\/api\.revenuecat\.com\/v\d\/subscribers\/,requires-body=1,script-path=https://raw.githubusercontent.com/nvnptit/wsrz/main/vsco.js

# ---- Từ module: aloha.module ----
AlohaVPN_Browser=type=http-response,pattern=https://api.alohaprofile.com/v1/profile_info,requires-body=1,script-path=https://raw.githubusercontent.com/nvnptit/wsrz/main/aloha.js

# ---- Từ module: locket.module ----
revenuecat = type=http-response, pattern=^https:\/\/api\.revenuecat\.com\/.+\/(receipts$|subscribers\/[^/]+$), script-path=https://raw.githubusercontent.com/nvnptit/wsrz/main/locket_pre.js, requires-body=true, max-size=-1, timeout=60
deleteHeader = type=http-request, pattern=^https:\/\/api\.revenuecat\.com\/.+\/(receipts|subscribers), script-path=https://raw.githubusercontent.com/nvnptit/wsrz/main/delHeader.js, timeout=60

# ==========================================================
# PHẦN HEADER REWRITE (TỪ SPOTIFY.MODULE)
# ==========================================================
[Header Rewrite]
http-request ^https:\/\/(spclient\.wg\.spotify\.com|.*-spclient\.spotify\.com(:443)?)\/user-customization-service\/v1\/customize$ header-del if-none-match

# ==========================================================
# PHẦN RULE (TỪ YOUTUBE.SGMODULE)
# ==========================================================
[Rule]
AND,((DOMAIN-SUFFIX,googlevideo.com), (PROTOCOL,UDP)),REJECT
AND,((DOMAIN,youtubei.googleapis.com), (PROTOCOL,UDP)),REJECT

# ==========================================================
# PHẦN URL REWRITE (TỪ YOUTUBE.SGMODULE)
# ==========================================================
https://ahrefs.com/writing-tools/paragraph-rewriter
(^https?:\/\/[\w-]+\.googlevideo\.com\/(?!dclk_video_ads).+?)&ctier=L(&.+?),ctier,(.+) $1$2$3 302
^https?:\/\/[\w-]+\.googlevideo\.com\/(?!(dclk_video_ads|videoplayback\?)).+&oad _ reject-200
^https?:\/\/(www|s)\.youtube\.com\/api\/stats\/ads _ reject-200
^https?:\/\/(www|s)\.youtube\.com\/(pagead|ptracking) _ reject-200
^https?:\/\/s\.youtube\.com\/api\/stats\/qoe\?adcontext _ reject-200